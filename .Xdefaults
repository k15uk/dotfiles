URxvt.saveLines: 65535
URxvt.scrollBar: false

! do not scroll with output
URxvt*scrollTtyOutput:   false
URxvt*scrollWithBuffer:  true
URxvt*scrollTtyKeypress: true

URxvt.termName: rxvt

URxvt.font: xft:Noto sans Mono CJK JP:pixelsize=14:antialias=true:bold=true
URxvt*xftAntialias: true
URxvt.letterSpace: -3

URxvt.depth: 32

! Colors
URxvt*background:     [70]#000000
URxvt*foreground:     #dcdccc
URxvt*cursorColor:    #ccffcc
URxvt*colorUL:        #dfaf8f
URxvt*underlineColor: #dfaf8f
URxvt*color0:         #3f3f3f
URxvt*color1:         #cc9393
URxvt*color2:         #7f9f7f
URxvt*color3:         #d0bf8f
URxvt*color4:         #6ca0a3
URxvt*color5:         #dc8cc3
URxvt*color6:         #93e0e3
URxvt*color7:         #dcdcdc
URxvt*color8:         #686868
URxvt*color9:         #dca3a3
URxvt*color10:        #bfebbf
URxvt*color11:        #f0dfaf
URxvt*color12:        #8cd0d3
URxvt*color13:        #dc8cc3
URxvt*color14:        #93e0e3
URxvt*color15:        #ffffff

URxvt*fading: 30

! 文字を表示するときに遅くなる
URxvt*skipBuiltinGlyphs: true

! perl拡張
URxvt.perl-ext-common: default,url-select,xim-onthespot,clipboard

! カット・アンド・ペースト with perl-ext-common "clipboard"
URxvt.clipboard.autocopy: true
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste

! クリックできるURL with perl-ext-common "default"
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

! ヤンクできるURL with perl-ext-common "default,url-select"
URxvt.perl-ext: default
URxvt.keysym.M-n: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open

! インプットメソッドのインライン表示 with perl-ext-common "xim-onthespot"
URxvt.inputMethod: uim
URxvt.preeditType: OnTheSpot

#include ".Xdefaults-local"
